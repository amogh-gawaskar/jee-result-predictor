{
  "version": 2,
  "builds": [
    {
      "src": "api/health/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/predict/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/colleges/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health/index.py"
    },
    {
      "src": "/api/predict",
      "dest": "/api/predict/index.py"
    },
    {
      "src": "/api/colleges",
      "dest": "/api/colleges/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/build/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|json|txt|woff|woff2|ttf|eot))",
      "dest": "/frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/index.html"
    }
  ]
}